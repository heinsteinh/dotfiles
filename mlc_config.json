{
  "ignorePatterns": [
    {
      "pattern": "^https://claude\\.ai"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 403],
  "httpHeaders": [
    {
      "urls": ["https://claude.ai"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ]
}
